   "outputs": [],
   "source": [
    "G = nx.DiGraph()\n",
    "edges = [(1, 2), (2, 3), (2, 1), (3, 2), (3, 4), (4, 1), (4, 2)]\n",
    "G.add_edges_from(edges)"
   ]
  },
